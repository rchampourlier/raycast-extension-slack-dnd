{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "slack-dnd",
  "title": "slack-dnd",
  "description": "Snooze notifications and set status in Slack at once",
  "icon": "command-icon.png",
  "author": "rchampourlier",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Slack DND",
      "subtitle": "Switch Do Not Disturb",
      "description": "Snooze notifications and set status in Slack at once",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@slack/web-api": "^6.8.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
